{
  "name": "@reactsax/reactsax",
  "version": "1.0.0-beta.1",
  "description": "Vuesax For Reacters",
  "author": "krishna-gujjjar",
  "keywords": [
    "react",
    "vuesax",
    "ui component",
    "react component",
    "react ui library",
    "components",
    "component",
    "javascript",
    "css",
    "framework",
    "ui",
    "frontend",
    "responsive",
    "sass",
    "design",
    "theme"
  ],
  "license": "ISC",
  "repository": "krishna-gujjjar/reactsax",
  "main": "dist/index.js",
  "module": "dist/index.modern.js",
  "source": "src/index.js",
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "build": "microbundle-crl --compress --format modern,cjs --css-modules false --no-sourcemap",
    "start": "microbundle-crl watch --compress --format modern,cjs --css-modules false --no-sourcemap",
    "storybook": "start-storybook",
    "build-storybook": "build-storybook -s public",
    "prepare": "run-s build",
    "test": "run-s test:unit test:lint test:build",
    "test:build": "run-s build",
    "test:lint": "eslint .",
    "test:unit": "cross-env CI=1 react-scripts test --env=jsdom",
    "test:watch": "react-scripts test --env=jsdom",
    "predeploy": "cd example && npm install && npm run build",
    "deploy": "gh-pages -d example/build",
    "docz:dev": "docz dev",
    "docz:build": "docz build",
    "docz:serve": "docz build && docz serve",
    "docz:clean": "docz clean"
  },
  "peerDependencies": {
    "react": "^16.0.0"
  },
  "devDependencies": {
    "@storybook/addon-actions": "^5.3.19",
    "@storybook/addon-docs": "^5.3.19",
    "@storybook/addon-knobs": "^5.3.19",
    "@storybook/addon-links": "^5.3.19",
    "@storybook/addon-storysource": "^5.3.19",
    "@storybook/preset-create-react-app": "^3.1.3",
    "@storybook/react": "^5.3.19",
    "acorn": "^7.3.1",
    "babel-eslint": "^10.0.3",
    "babel-loader": "^8.1.0",
    "cross-env": "^7.0.2",
    "docz": "^2.3.1",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.7.0",
    "eslint-config-standard": "^14.1.0",
    "eslint-config-standard-react": "^9.2.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-node": "^11.0.0",
    "eslint-plugin-prettier": "^3.1.1",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-react": "^7.17.0",
    "eslint-plugin-standard": "^4.0.1",
    "gatsby-plugin-sass": "^2.3.12",
    "gh-pages": "^2.2.0",
    "microbundle-crl": "^0.13.10",
    "node-sass": "^4.14.1",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.0.4",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-is": "^16.13.1",
    "react-refresh": "^0.8.3",
    "react-scripts": "^3.4.1",
    "sass": "^1.26.10"
  },
  "files": [
    "dist"
  ]
}
